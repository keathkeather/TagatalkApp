  {
    "expo": {
      "name": "Tagatalk",
      "scheme": "com.whitelotus.tagatalk",
      "plugins": [
        "expo-router",
        [
          "expo-av",
          {
            "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
          }
        ],
        "expo-font",
        "expo-secure-store"
      ],
      "experiments": {
        "typedRoutes": true,
        "tsconfigPaths": true
      },
      "orientation": "portrait",
      "icon": "./assets/app_logo.png",
      "userInterfaceStyle": "light",
      "splash": {
        "image": "./assets/ssplash.png",
        "resizeMode": "contain",
        "backgroundColor": "#FD9F10"
      },
      "assetBundlePatterns": [
        "**/*"
      ],
      "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.whitelotus.tagatalk"
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/app_logo.png",
          "backgroundColor": "#FD9F10"
        },
        "permissions": [
          "android.permission.RECORD_AUDIO",
          "android.permission.MODIFY_AUDIO_SETTINGS"
        ],
        "package": "com.whitelotus.tagatalk"
      },
      "extra": {
        "router": {
          "origin": false
        },
        "eas": {
          "projectId": "37baa903-928e-4cd2-bc00-85d5e6fa53f5"
        }
      },
      "runtimeVersion": {
        "policy": "appVersion"
      },
      "updates": {
        "url": "https://u.expo.dev/37baa903-928e-4cd2-bc00-85d5e6fa53f5"
      }
    }
  }
